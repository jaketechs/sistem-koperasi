{
  "name": "Sistem Manajemen Koperasi",
  "version": "1.0.0", 
  "description": "Aplikasi web untuk manajemen keuangan Koperasi Pasar Padang Pangan",
  "main": "main.html",
  "keywords": [
    "koperasi",
    "manajemen keuangan", 
    "neraca",
    "kas",
    "laporan keuangan",
    "web application"
  ],
  "author": "Tim Developer KKN Tematik",
  "license": "MIT",
  "homepage": "https://github.com/username/koperasi",
  "repository": {
    "type": "git",
    "url": "https://github.com/username/koperasi.git"
  },
  "dependencies": {
    "bootstrap": "^5.3.0",
    "font-awesome": "^6.4.0"
  },
  "scripts": {
    "start": "live-server --port=8080",
    "dev": "live-server --port=3000 --watch=assets/"
  },
  "browser": {
    "main.html": "."
  },
  "features": [
    "Dashboard dengan ringkasan keuangan",
    "Input neraca harian dengan kategori lengkap",
    "Manajemen kas pemasukan dan pengeluaran", 
    "Laporan keuangan bulanan",
    "CRUD data anggota koperasi",
    "Export/Import data",
    "Responsive design untuk laptop/desktop"
  ],
  "technical_specs": {
    "frontend": "HTML5, CSS3, JavaScript ES6+",
    "framework": "Bootstrap 5.3.0",
    "icons": "Font Awesome 6.4.0",
    "storage": "localStorage",
    "browser_support": [
      "Chrome >= 80",
      "Firefox >= 75", 
      "Safari >= 13",
      "Edge >= 80"
    ]
  },
  "data_structure": {
    "neraca": {
      "fields": [
        "tanggal",
        "kasMasuk", 
        "piutangPokok",
        "piutangJasa",
        "danaisPokok", 
        "danaisJasa",
        "simpananPokok",
        "simpananWajib",
        "simpananSukarela",
        "keteranganLain",
        "jumlahLain",
        "total"
      ]
    },
    "kas": {
      "fields": [
        "tanggal",
        "jenis",
        "jumlah", 
        "keterangan"
      ]
    },
    "anggota": {
      "fields": [
        "noAnggota",
        "nama",
        "telepon",
        "tanggalBergabung",
        "alamat",
        "status"
      ]
    }
  }
}
