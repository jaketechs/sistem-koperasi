{
  "version": 2,
  "name": "sistem-koperasi",
  "builds": [
    {
      "src": "main.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/",
      "dest": "/main.html"
    }
  ],
  "functions": {
    "start-backend.js": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false
}
